{
  "name": "@assets-release-kit/cli",
  "packageManager": "yarn@4.9.1",
  "version": "0.0.0",
  "type": "module",
  "bin": {
    "ark-cli": "./bin/cli-dev.js"
  },
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
